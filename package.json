{
  "name": "rs-lab",
  "version": "0.0.1",
  "description": "lab for rollingsagas",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-storybook": "storybook build",
    "pages:build": "pnpm prisma generate && npx @cloudflare/next-on-pages",
    "preview": "pnpm run pages:build && wrangler pages dev",
    "deploy": "pnpm run pages:build && wrangler pages deploy"
  },
  "keywords": [
    "rollingsagas",
    "lab.rollingsagas.com"
  ],
  "author": "aspirin2d@outlook.com",
  "license": "MIT",
