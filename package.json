    "@hugeicons/react": "^0.5.2",
    "@uiw/codemirror-extensions-langs": "^4.23.0",
    "@uiw/codemirror-theme-tokyo-night": "^4.23.0",
    "@uiw/codemirror-theme-tokyo-night-day": "^4.23.0",
    "@uiw/react-codemirror": "^4.23.0",
    "axios": "^1.7.8",
    "dayjs": "^1.11.12",
    "js-cookie": "^3.0.5",
