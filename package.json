{
  "name": "rs-lab",
  "version": "0.0.1",
  "description": "lab for rollingsagas",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm next dev",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "lint": "pnpm next lint",
    "pages:build": "pnpm prisma generate && npx @cloudflare/next-on-pages@1",
    "preview": "pnpm run pages:build && wrangler pages dev",
    "deploy": "pnpm run pages:build && wrangler pages deploy"
  },
  "keywords": [
